{
    "name": "@prefecthq/orion-design",
    "version": "0.1.10",
    "private": false,
    "keywords": [
        "prefect",
        "vue3",
        "orion"
    ],
    "scripts": {
        "dev": "vite build --watch && npm run dev-components && npm run dev-models && npm run dev-services && npm run dev-utilities",
        "build": "vue-tsc && vite build && npm run build-components && npm run build-models && npm run build-services && npm run build-utilities",
        "build-components": "vite build --mode components",
        "build-models": "vite build --mode models",
        "build-services": "vite build --mode services",
        "build-utilities": "vite build --mode utilities",
        "dev-components": "vite build --mode components --watch",
        "dev-models": "vite build --mode models --watch",
        "dev-services": "vite build --mode services --watch",
        "dev-utilities": "vite build --mode utilities --watch",
        "postbuild": "node scripts/fix-types.js"
    },
    "main": "./dist/orion-design.umd.js",
    "module": "./dist/orion-design.es.js",
    "exports": {
        ".": {
            "import": "./dist/orion-design.es.js",
            "require": "./dist/orion-design.umd.js"
        },
        "./components":{
            "import": "./dist/orion-design-components.es.js",
            "require": "./dist/orion-design-components.umd.js"
        },
        "./models":{
            "import": "./dist/orion-design-models.es.js",
            "require": "./dist/orion-design-models.umd.js"
        },
        "./services":{
            "import": "./dist/orion-design-services.es.js",
            "require": "./dist/orion-design-services.umd.js"
        },
        "./utilities":{
            "import": "./dist/orion-design-utilities.es.js",
            "require": "./dist/orion-design-utilities.umd.js"
        },
        "./dist/style.css": "./dist/style.css"
    },
    "files": [
        "dist"
    ],
    "types": "./dist/index.d.ts",
    "dependencies": {
        "@prefecthq/miter-design": "0.1.20",
        "@prefecthq/vue-compositions": "0.1.9",
        "axios": "^0.24.0",
        "core-js": "^3.6.5",
        "d3": "^7.0.0",
        "vue": "^3.0.5",
        "vuex": "^4.0.2"
    },
    "devDependencies": {
        "@babel/helper-compilation-targets": "^7.16.3",
        "@babel/runtime": "^7.16.3",
        "@prefecthq/eslint-config": "1.0.11",
        "@types/d3": "^7.0.0",
        "@types/node": "^15.14.1",
        "@vitejs/plugin-vue": "^1.2.5",
        "date-fns": "^2.25.0",
        "eslint": "^8.6.0",
        "replace": "^1.2.1",
        "sass": "^1.35.2",
        "typescript": "^4.1.6",
        "vite": "^2.4.2",
        "vue-class-component": "^8.0.0-0",
        "vue-tsc": "^0.30.1"
    }
}
